#include "main.h"
/**
* _strncpy - copy string
* @dest: string 1
* @src: string 2
* @n: input 3
* Return: value of dest
*/

char *_strncpy(char *dest, char *src, int n)
{
int x = 0;
int y = 0;
while (x < n && src[x] != '\0' && dest[y] < 98)
{
x++;
y++;
dest[y] = src[x];
}
while (x < n)
{
x++;
dest[y] = '\0';

}
return (dest);
}
