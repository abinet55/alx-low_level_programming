#include "main.h"
/**
* _strncat - concatenate two strings
* @dest: destination string
* @src: source string
* @n: input integer
* Return: destination
*/

char *_strncat(char *dest, char *src, int n)
{
int d = 0;
int s = 0;

 for (d = 0; dest[d] != '\0'; d++)
   {
     for (s = 0; src[s] != '\0'; s++)
dest[s] = '\0';
return (dest);
}
